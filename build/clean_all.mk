.PHONY: clean
clean:
	$(QUIET)rm -f $(all_objects) $(all_dependencies) $(DEVNUL)
	$(QUIET)rm -f $(all_targets) $(DEVNUL)

.PHONY: distclean
distclean:
	-$(QUIET)rm -rf * $(DEVNUL)
